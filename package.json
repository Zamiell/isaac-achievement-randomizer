{
  "name": "isaac-achievement-randomizer",
  "version": "0.14.8",
  "description": "A mod for The Binding of Isaac: Repentance.",
  "keywords": [
    "isaac",
    "mod",
    "rebirth",
    "afterbirth",
    "repentance"
  ],
  "license": "GPL-3.0",
  "type": "module",
  "scripts": {
    "build": "tstl",
    "build-website": "esbuild ./website/src/main.ts --bundle --outfile=./website/static/main.js --sourcemap",
    "cloc": "cloc ./src --include-lang=TypeScript",
    "lint": "tsx ./scripts/lint.ts",
    "nuke": "isaacscript nuke",
    "publish": "isaacscript publish",
    "start": "isaacscript monitor",
    "update": "isaacscript update",
    "watch": "esbuild ./website/src/main.ts --bundle --outfile=./website/static/main.js --sourcemap --watch"
  },
  "dependencies": {
    "isaac-lua-polyfill": "^1.1.1",
    "isaac-typescript-definitions": "^41.0.0",
    "isaacscript-common": "^87.2.2",
    "isaacscript-common-ts": "^16.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/source-map-support": "^0",
    "cloc": "^2.0.0-cloc",
    "esbuild": "^0.20.1",
    "isaacscript": "^4.2.2",
    "isaacscript-common-node": "^3.0.1",
    "isaacscript-lint": "^6.20.0",
    "isaacscript-spell": "^1.14.0",
    "isaacscript-tsconfig": "^5.0.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "typescript-to-lua": "^1.24.1"
  },
  "packageManager": "yarn@4.1.0"
}
