{
  "name": "isaac-achievement-randomizer",
  "version": "0.13.0",
  "description": "A mod for The Binding of Isaac: Repentance.",
  "keywords": [
    "isaac",
    "mod",
    "rebirth",
    "afterbirth",
    "repentance"
  ],
  "license": "GPL-3.0",
  "type": "module",
  "scripts": {
    "build": "tstl",
    "build-website": "esbuild ./website/src/main.ts --bundle --outfile=./website/static/main.js",
    "cloc": "cloc ./src --include-lang=TypeScript",
    "lint": "tsx ./scripts/lint.ts",
    "nuke": "isaacscript nuke",
    "publish": "isaacscript publish",
    "start": "isaacscript monitor",
    "update": "isaacscript update"
  },
  "dependencies": {
    "isaac-typescript-definitions": "^39.0.5",
    "isaacscript-common": "^80.2.5",
    "isaacscript-common-ts": "^11.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "cloc": "^1.98.0-cloc",
    "esbuild": "^0.19.8",
    "isaacscript": "^3.27.0",
    "isaacscript-common-node": "^1.4.0",
    "isaacscript-lint": "^6.4.1",
    "isaacscript-spell": "^1.10.0",
    "isaacscript-tsconfig": "^5.0.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2",
    "typescript-to-lua": "^1.22.0"
  },
  "packageManager": "yarn@4.0.2"
}
